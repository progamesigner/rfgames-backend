{
    "name": "rfgames-api-backends",
    "version": 2,
    "env": {
        "APPLY_WEBHOOK_URL": "@apply-webhook-url",
        "CONTACT_WEBHOOK_URL": "@contact-webhook-url"
    },
    "functions": {
        "api/**/*.rs": {
            "memory": 128,
            "runtime": "now-rust@1.0.1"
        }
    },
    "rewrites": [
        {
            "source": "/v1/(.*)",
            "destination": "/api/$1"
        }
    ]
}
